{
  "name": "react-pagination",
  "version": "1.0.0",
  "description": "It is a simple react pagination which also supports RWD.",
  "scripts": {
    "production": "webpack --config ./webpack.prod.config.js",
    "dev": "webpack-dev-server --devtool eval --progress --colors --content-base",
    "eslint": "eslint src --fix",
    "test:watch": "jest test --watch",
    "test": "jest test"
  },
  "jest": {
    "modulePaths": [
      "src/js"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./test/enzyme_adapter.js"
  },
  "repository": {
    "type": "git",
    "url": "react-pagination"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "graphql"
  ],
  "author": "Jocelyn",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "node-sass": "^7.0.0",
    "react-hot-loader": "^3.1.1",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.0.1",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  }
